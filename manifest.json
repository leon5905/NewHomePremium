{
	"manifest_version": 2,

	"name": "sample",
	"description": "sample",
	"version": "0.1",
    
	"chrome_url_overrides": {
		"newtab": "momentum.html"
	},

	"background": {
		"scripts": ["clock/clock-1.1.0.min.js", "momentum.js","clock.js"]
	},

	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';",

	"permissions": [
		"geolocation",
		"storage",
		"unlimitedStorage"
	]
}